<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="guestbook">
	<select id="getList">
		<![CDATA[
		select no,
			   name,
			   content,
			   up_date,
			   passWord 
			from guestbook 
				order by no asc
		]]>
	</select>
</mapper>